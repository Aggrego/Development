{
  "require-dev": {
    "wikimedia/composer-merge-plugin": "1.4.*"
  },
  "extra": {
    "merge-plugin": {
      "include": [
        "/app/REPLACE/composer.json",
        "/Development/repositories.json"
      ],
      "recurse": true,
      "replace": false,
      "ignore-duplicates": false,
